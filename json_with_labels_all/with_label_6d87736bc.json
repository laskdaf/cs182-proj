    "sentences": [
        {
            "text": {
                "content": "This is Fuzzy, she is the Daughter of Digit who kind of moved into our office house.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "Fuzzy is a quiet little girl who like to do her own thing and look pretty, it's the Persian in her.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "Since she is being homed here at my office we unfortunately can't keep her.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": -0.2
            }
        }, 
        {
            "text": {
                "content": "We are look for a good home for her.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.7, 
                "score": 0.7
            }
        }, 
        {
            "text": {
                "content": "Please note that she is an indoor kitten and has never been outdoors, so I would prefer that she be adopted by someone who can provide an indoor setting.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "I'm also willing to have her spayed at my expense if the adopter requests.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.5, 
                "score": 0.5
            }
        }, 
        {
            "text": {
                "content": "Please contact either James (English only) or Justin (English &amp; Malay) if your interested in adopting Fuzzy", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }
    ], 
