    "sentences": [
        {
            "text": {
                "content": "I adopted a stray unspayed female.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Before we manage to get her spayed she got pregnant.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Since to opt for abortion seemed kind of inhumane at the stage of her pregnancy (when we realized she was pregnant), we allowed her to have her kittens.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": -0.9
            }
        }, 
        {
            "text": {
                "content": "However due to space(we currently have 8 cats) and time constrains(I will be going to university soon), we are forced to put 2 kittens up for adoption.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "No fees are required as long as a good home is provided for the kittens.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": 0.8
            }
        }, 
        {
            "text": {
                "content": "You may choose to adopt only one of them or you may take in both, if possible.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "Contact Shaunie () or Cheryl () for more details.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }
