    "documentSentiment": {
        "magnitude": 3.3, 
        "score": 0.1
    }, 
    "sentences": [
        {
            "text": {
                "content": "We found her when she was almost on the verge of death, with her lower jaw fractured in 2 places.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "We had her undergo a surgery, and now she is on the road to recovery.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "She has been vaccinated, and all her surgery costs paid for.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "As she grows into a beautiful cat, her jaw will heal back to normal, and according to the vet, she will be able to eat normally.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.5, 
                "score": 0.5
            }
        }, 
        {
            "text": {
                "content": "As of now, no medication is needed anymore, just letting healing takes place.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": -0.2
            }
        }, 
        {
            "text": {
                "content": "As she recovers, she needs your TLC.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "And she is no different from any other cute kitten.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "Very gentle and sweet in nature, all she needs is your TLC.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": 0.8
            }
        }, 
        {
            "text": {
                "content": "Please kindly consider to adopt her, or please foster her until a suitable adopter comes along.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.5, 
                "score": 0.5
            }
        }, 
        {
            "text": {
                "content": "My family and I will not be able to foster her any longer due to certain issues, please bring her with you as we do not want to surrender her to a shelter.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.5, 
                "score": -0.5
            }
        }
    ], 
