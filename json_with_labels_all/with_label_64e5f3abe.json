    "sentences": [
        {
            "text": {
                "content": "Sally was a stray cat found near my office in Damansara Uptown.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "I took her to a nearby vet &amp; had her neutered, dewormed &amp; vaccinated.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Soon after that she had fever &amp; diarrhoea and I brought her to Gasing Hospital where she was admitted for roughly 1 week.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.7, 
                "score": -0.7
            }
        }, 
        {
            "text": {
                "content": "That was in December .", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Now after several checkups she's fine and has recently had her 2nd vaccination.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "Sally is naughty, active and very very playful!", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.7, 
                "score": 0.7
            }
        }, 
        {
            "text": {
                "content": "Please give her a forever home =)", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.5, 
                "score": 0.5
            }
        }
