    "documentSentiment": {
        "magnitude": 6.5, 
        "score": 0.2
    }, 
    "sentences": [
        {
            "text": {
                "content": "Many thanks to Miss Jasmine from Penang to adopt COCO and promising to give her a new sweet home!", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": 0.8
            }
        }, 
        {
            "text": {
                "content": "I picked up Coco from SPCA 2 weeks after I got another dog Fifi.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "I'm not very familiar with type of breed as I'm no expert, but according to the SPCA guys, she is a Labrador mix.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "5 months, she grew rapidly.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "She's a bit heavier compared to my other dog and she has strong thigh.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "She's a loving puppy and most of the times will be playing on her own.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "She is toilet trained, poo only outside the house and pee only in toilet.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.5, 
                "score": -0.5
            }
        }, 
        {
            "text": {
                "content": "She loves to ride in car with air blowing to her face.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "Coco feds only on Purina Dried Food and canned food on weekends.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": -0.8
            }
        }, 
        {
            "text": {
                "content": "Unfortunately, Coco is now alone at home as Fifi has been adopted by a new good owner with someone to accompany her all the time.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.3, 
                "score": 0.3
            }
        }, 
        {
            "text": {
                "content": "I hope to find a good owner for Coco as well as I love both of them.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": 0.8
            }
        }, 
        {
            "text": {
                "content": "Reason for me to let go is due to my career which requires me to travel very often now.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "I have no choice but to let her go with a heavy heart.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "You can write to me to Nicholas.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "or sms me at (I\u2019m in meeting most of the time and wont be answering) or BB Pin 21BEE.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": -0.2
            }
        }
    ], 
    "categories": []
}