    "sentences": [
        {
            "text": {
                "content": "Shorty as we fondly call him, is the little doggy camping out at our place.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "The owner abandoned him when they vacated their house, leaving him in the house compound.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.6, 
                "score": -0.6
            }
        }, 
        {
            "text": {
                "content": "I am currently taking care of it.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "Due to time constraint and limited space we are looking to give him to someone who can take good care of him.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "Very playful fellow and needs lots of attention, He must be only 2 years old or so.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": 0.8
            }
        }, 
        {
            "text": {
                "content": "Perfectly healthy, no ticks, skin problem or any disabilities.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": 0.8
            }
        }, 
        {
            "text": {
                "content": "Let me know if you know anybody suitable that would wish to adopt him.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.5, 
                "score": 0.5
            }
        }
