    "sentences": [
        {
            "text": {
                "content": "Lucky was found in a car engine when the car went for servicing.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "When the mechanics checked the engine they found two kittens.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Unfortunately, his sibling was already dead - injured by the fan belt.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": -0.4
            }
        }, 
        {
            "text": {
                "content": "Upon checking with the owner of the car, the owner said the kittens were not his.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.3, 
                "score": -0.3
            }
        }, 
        {
            "text": {
                "content": "Therefore, now Lucky, who is a very healthy, playful kitten is with us.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": 0.8
            }
        }, 
        {
            "text": {
                "content": "He eats on his own, and has learned to use the litter tray.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.7, 
                "score": 0.7
            }
        }, 
        {
            "text": {
                "content": "If you are interested to adopt him please contact me at, or e-mail me.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }
    ], 
