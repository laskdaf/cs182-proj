    "sentences": [
        {
            "text": {
                "content": "I have a 2 year old miniature poodle with me but due to my assignments, the work schedule is crazy.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "My dog has been neglected so I have decided to give him to a better family to take care of him.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": -0.4
            }
        }, 
        {
            "text": {
                "content": "He is an indoor dog so please dont leave him outside the house.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Casper is a very active dog.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "He likes to play with ball and will play fetch with the owner.", 
