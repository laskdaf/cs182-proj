    "documentSentiment": {
        "magnitude": 4.9, 
        "score": 0.1
    }, 
    "sentences": [
        {
            "text": {
                "content": "Rescued at one month, she has been with us for a year.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "I just could not let her go, but, the time has come for a permanent home.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "She is the smartest dog I have ever worked with.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.7, 
                "score": 0.7
            }
        }, 
        {
            "text": {
                "content": "She is timid, but once she knows you she is loving and devoted.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "She knows both verbal commands and ten words in American Sign Language.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "Again, she is very smart.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "She is cage trained and will nap and sleep in the cage if the door is left open.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "She spends the night in the cage.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "She is house broken.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.7, 
                "score": -0.7
            }
        }, 
        {
            "text": {
                "content": "You will need to spend a few days taking her out on a regular schedule [after meals] so she gets used to the new environment.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "All vaccinations have been given.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "She has been dewormed and has taken heartworm medicine and flee/tic medicine every month.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": -0.2
            }
        }, 
        {
            "text": {
                "content": "She is in a condo now but I suspect she would love a yard.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "She is very excited to be outside.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.5, 
                "score": 0.5
            }
        }, 
        {
            "text": {
                "content": "Please let me know if you have any questions.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "Pictures to follow.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }
    ], 
    "categories": []
