    "sentences": [
        {
            "text": {
                "content": "Alex was placed in a box outside my door.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "He had some fleas on him which we took care of it and now he is free from fleas.", 
