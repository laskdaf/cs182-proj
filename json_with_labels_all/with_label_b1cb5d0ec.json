    "sentences": [
        {
            "text": {
                "content": "Introducing little Black Jack!", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "----------------------------------------------------- \u2665 Male kitten, 6 weeks old, round-faced boy with a handsome marbled-coat.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.6, 
                "score": 0.6
            }
        }, 
        {
            "text": {
                "content": "\u2665 Curious character, not hyper nor too adventurous.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "Likes his own spots (for eating, sleeping, etc).", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "\u2665 Extremely &quot;manja&quot; and VERY vocal, will purr away during cuddle-time.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.5, 
                "score": 0.5
            }
        }, 
        {
            "text": {
                "content": "He will &quot;talk&quot; and tell you without fail just how much he adores you! :)", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "\u2665 Clever with the litter-box.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.7, 
                "score": 0.7
            }
        }, 
        {
            "text": {
                "content": "Dewormed (once) and given flea prevention.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Currently too young to be vaccinated.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "----------------------------------------------------- ***PLEASE READ*** (NOTE FOR ADOPTERS): \u2665 Black Jack must be kept INDOORS at all times (if apartments, must be cat-proofed).", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "\u2665 Adopters must agree to NEUTER him at the right age (please inquire regarding fee).", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "\u2665 Premium food feeding only (no Whiskas, Friskies etc).", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "----------------------------------------------------- Please drop me an email or call if you feel that you may be just the right person to take this boy home.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Thank you! :)", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }
