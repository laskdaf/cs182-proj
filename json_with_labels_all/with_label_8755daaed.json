    "sentences": [
        {
            "text": {
                "content": "Buster was rescued by my husband's colleague in the office parking lot.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "With one eye dangling out, he was rushed to the hospital where the kind doctors there did surgery on his eye for only RM (The actual surgery cost 1k++!!)", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.6, 
                "score": -0.6
            }
        }, 
        {
            "text": {
                "content": "He's been a little trooper adjusting well to his one eye.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Extremely energetic but has been taught how to play by himself.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "For RM (Negotiable) we will provide: Kitty bed, Litter box, some litter and food, one food dish, kitty stand and all his toys.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "He's already had his first month's vaccination.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "I would like to encourage serious adopters who would ensure his well being and keep up the vaccination and neutering process.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.7, 
                "score": 0.7
            }
        }, 
        {
            "text": {
                "content": "Available immediately.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "For adoption please Whatsapp.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }
