    "sentences": [
        {
            "text": {
                "content": "Mr Sam Lau of ACT Sabah (Animal Concern Team Sabah) saw my post in FB (linked to petfinder) on little Chicil and mentioned that he'd found an adopter.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.7, 
                "score": 0.7
            }
        }, 
        {
            "text": {
                "content": "I am happy to report that Chicil has been taken by Mr Sam Lau on 2/1/ to be handed over to his adopter.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.6, 
                "score": 0.6
            }
        }, 
        {
            "text": {
                "content": "Chicil is a smart kitty.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "I hope that he will enjoy his new home.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.6, 
                "score": 0.6
            }
        }, 
        {
            "text": {
                "content": "No more wandering around the neighborhood begging for food :)", 
