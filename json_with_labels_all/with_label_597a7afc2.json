    "sentences": [
        {
            "text": {
                "content": "I found this little guy at Menara Duta crying and scratching his ears.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Someone must've abandoned him, because he came right up to me and rested his head on my shoe.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": -0.2
            }
        }, 
        {
            "text": {
                "content": "I immediately took him to the vet.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "He's now had a shot for his scabies/mange and needs to go back once a week for the next three weeks.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.3, 
                "score": -0.3
            }
        }, 
        {
            "text": {
                "content": "He's had a de-worming pill and I've been giving him multivitamins the vet provided.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "He's healing quite nicely and has started playing and exploring finally.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "It took him three attempts but he's now pooping in the litter box.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.6, 
                "score": -0.6
            }
        }, 
        {
            "text": {
                "content": "He's quite affectionate and playful and would make a great addition to any family.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }
