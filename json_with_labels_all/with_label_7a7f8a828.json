    "sentences": [
        {
            "text": {
                "content": "Lil Miss Stevie is a young mum to 2 beautiful kitties who are only a month old.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.5, 
                "score": 0.5
            }
        }, 
        {
            "text": {
                "content": "Both kittens are female, one is a spitting image of her mum with a black and white coat and the other is of white and grey (she has a perfect circle of grey on her back!).", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "We found them hiding in a bush outside of our condo and are looking to find them a loving home.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "We would love to keep them ourselves as Stevie is so affectionate but unfortunately she is not too fond of our puppy.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "Please contact me if you would like to adopt this cute furry family of 3 girls.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }
