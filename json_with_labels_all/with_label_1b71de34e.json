    "documentSentiment": {
        "magnitude": 4.7, 
        "score": 0.4
    }, 
    "sentences": [
        {
            "text": {
                "content": "Hi, I'm looking for a forever home for my two ginger boys, Nemo &amp; Tigger.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.6, 
                "score": 0.6
            }
        }, 
        {
            "text": {
                "content": "I have too many cats at home and after much consideration, I've decided to let both brothers go because they're the most affectionate among my many cats and they absolutely love being in the company of people.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "They're the sort of cats who would drop to the floor when they meet anyone and roll on their backs begging for tummy rubs.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": -0.4
            }
        }, 
        {
            "text": {
                "content": "They're also very vocal and we've spent many nights and days having such great conversation.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "They're an absolute delight for companions!", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "They're very attached to one another which is why they can ONLY be adopted together as I can't bear to have the brothers separated.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": -0.2
            }
        }, 
        {
            "text": {
                "content": "If you have any further enquiries, please don't hesitate to drop me a message on WhatsApp :)", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }
    ], 
    "categories": []
}