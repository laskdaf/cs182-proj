    "sentences": [
        {
            "text": {
                "content": "Saved this little kitten from the middle of the road from on-coming traffic.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.3, 
                "score": 0.3
            }
        }, 
        {
            "text": {
                "content": "Its sibling was already dead after being run over by cars.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "Due to the rainy season, it caught a flu &amp; was too weak to fend for itself.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.6, 
                "score": -0.6
            }
        }, 
        {
            "text": {
                "content": "Hence, I brought it to the vet &amp; is currently nursing it back to health.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Its really tiny (approximately the size of my palm).", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "It is relatively well behaved, require very little space &amp; sleeps in a carton box.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "I hope someone kind can give this kitten a warm and loving home.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "I won't be able to keep it for very long because I'm currently housing 5 dogs in my house.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": -0.4
            }
        }, 
        {
            "text": {
                "content": "Drop me an email if u are interested.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
