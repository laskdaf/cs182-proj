    "sentences": [
        {
            "text": {
                "content": "12 soldiers were rescued from Puchong dog pound by independent rescuer Michelle.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "They were the litter of a mama dog used to be fed by Michelle.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "All soldiers were caught and left with the mama dog waiting for her babies.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Michelle took the decision to claim them back from the pound.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "They are all very healthy.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "All babies are under quarantined at the moment for at least 1 week.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "After that they will brought to the vet for deworming and vaccination.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Please help Michelle to find a home for all the cute soldiers.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "Or at least somebody please help to foster them while we work together to look for adopters.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "Those who are interested, please call Michelle at.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "Adopter must be willing to spay and neuter the puppy upon the right age.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "There are 6 male and 6 female all together.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }
    ], 
    "categories": []
}