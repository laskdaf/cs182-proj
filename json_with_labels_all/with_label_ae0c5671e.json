    "sentences": [
        {
            "text": {
                "content": "I have a 6 years plus Siberian Husky, named Owen, up for adoption.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Other than yeast infection (under treatment) under his neck, Owen is a perfectly healthy dog.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.7, 
                "score": 0.7
            }
        }, 
        {
            "text": {
                "content": "My new current work portfolio doesn't allow me to spend much time with Owen as i should.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.3, 
                "score": -0.3
            }
        }, 
        {
            "text": {
                "content": "I hope the potential adopter will be able to care for Owen and help him to live the way he should be.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.3, 
                "score": 0.3
            }
        }, 
        {
            "text": {
                "content": "Would prefer the adopter (and not any other rep) to be from Klang Valley region as i may want to pay him a visit on and off.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.3, 
                "score": 0.3
            }
        }, 
        {
            "text": {
                "content": "You may get in touch with me for more details.", 
