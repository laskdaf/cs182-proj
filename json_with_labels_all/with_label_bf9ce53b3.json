    "documentSentiment": {
        "magnitude": 4.7, 
        "score": 0.4
    }, 
    "sentences": [
        {
            "text": {
                "content": "Hi everyone, My name is Bubu, and we're a family of eight adorable cats.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": 0.8
            }
        }, 
        {
            "text": {
                "content": "We're looking for a new home since our human mummy told us that she need to pursue her study abroad for three years.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "No one will take care of us.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": -0.2
            }
        }, 
        {
            "text": {
                "content": "There is no doubt that our mummy love us so much, but looking for a new family for us is the best option she had.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "We tried to convince her that relocating a domestic cat like us is not easy, but my mummy believed there will be a kind hearted people out there who are willing to give us a place to call home again.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": -0.4
            }
        }, 
        {
            "text": {
                "content": "This is by far the most difficult decision she's ever made because she love us very much.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.6, 
                "score": 0.6
            }
        }, 
        {
            "text": {
                "content": "I hope, you will give us chance to be part of your family, and we promise to become a good companion and we'll love you with all of our heart.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "Kind regards, Bubu and the family", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }
    ], 
    "categories": []
