    "sentences": [
        {
            "text": {
                "content": "Klang Valley only.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "Mr Dido and his friend, Roger, were found in the middle of a road in Klang by a friend on 28 sept .", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.3, 
                "score": 0.3
            }
        }, 
        {
            "text": {
                "content": "He nearly ran over them.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Both of them were very dirty and fur all matted up.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": -0.9
            }
        }, 
        {
            "text": {
                "content": "Have since been groomed.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "Mr Dido underwent a minor surgery to clean up a wound near his eyes.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.3, 
                "score": 0.3
            }
        }, 
        {
            "text": {
                "content": "He has recovered beautifully and is a quiet, good boy.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "He loves children and is very affectionate.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "Gets along with other dogs and is very obedient.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.6, 
                "score": 0.6
            }
        }, 
        {
            "text": {
                "content": "Weighs 7 kgs.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Indoor dog.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.3, 
                "score": 0.3
            }
        }, 
        {
            "text": {
                "content": "No tying up or caging please.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": -0.2
            }
        }, 
        {
            "text": {
                "content": "Do call me if you can give him a good home.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.7, 
                "score": 0.7
            }
        }
    ], 
