    "sentences": [
        {
            "text": {
                "content": "Pooch was found by my mother being kept under very poor conditions in enclosed cement barrel with unsuitable food and no water (only cow's milk which is bad for puppies).", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": -0.9
            }
        }, 
        {
            "text": {
                "content": "Pooch has a bad case of mange and was initially pooping a lot of watery discharge.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.6, 
                "score": -0.6
            }
        }, 
        {
            "text": {
                "content": "He was also initially very frightened.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "He was treated with mange medication for over a month and is now a healthy, happy and very active puppy.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "To know more about his story, please visit my blog here: If interested to adopt and give this happy pup a good home, please contact my mother Jocelyn at.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "Thank you!", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }
