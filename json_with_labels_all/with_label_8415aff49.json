    "sentences": [
        {
            "text": {
                "content": "Musang was born at my house.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "When she was 4-mth old, she suffered serious injury as a result of an attack.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": -0.4
            }
        }, 
        {
            "text": {
                "content": "It really scared the whole household.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "Luckily, after adequately attended and medication, she recovered to her oldself.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "What a relief...", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "But soon I'll need to move to a new residence, I can't bring her along.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "I'm looking for somebody who would adopt her and love her.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": 0.8
            }
        }, 
        {
            "text": {
                "content": "I'll miss her, for sure...", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }
    ], 
