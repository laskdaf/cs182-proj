    "sentences": [
        {
            "text": {
                "content": "I first rescue the pregnant mother and give her a shelter to give birth to her litters.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.4, 
                "score": 0.4
            }
        }, 
        {
            "text": {
                "content": "There are 7 siblings in total.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "All the litters stayed in my house and are very tame and mild tempered.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "The mother is a very responsible one and all her litters are well-behaved.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.2, 
                "score": 0.2
            }
        }, 
        {
            "text": {
                "content": "I sincerely hope that the adopter can really promise me to take great care of them and LOVE them like you love yourself if not more.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": 0.8
            }
        }, 
        {
            "text": {
                "content": "Guan Yin gets her name because she has got a yellow mark on her forehead, like the buddha guan yin.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.5, 
                "score": 0.5
            }
        }, 
        {
            "text": {
                "content": "Guan Yin is now almost 6 weeks old.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }
