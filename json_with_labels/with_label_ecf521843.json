    "sentences": [
        {
            "text": {
                "content": "This black beauty is a friendly stray at my area.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": 0.8
            }
        }, 
        {
            "text": {
                "content": "He had lost an eye, probably due to fights with other strays, but he seems fine, and not bothered by this old injury.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "Generally he's very healthy.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
        }, 
        {
            "text": {
                "content": "He usually hangs out at my shop, so we provide food &amp; shelter for him.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": -0.1
            }
        }, 
        {
            "text": {
                "content": "He's very playful, loyal and protective.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.9, 
                "score": 0.9
            }
        }, 
        {
            "text": {
                "content": "We hope someone will be kind enough to give this dog a happy home.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.8, 
                "score": 0.8
            }
        }, 
        {
            "text": {
                "content": "Please note that I work at Cameron Highlands, so anyone who is willing to come and adopt him should allocate a time to come and pick him.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Please contact me at or email to earlier.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0, 
                "score": 0
            }
        }, 
        {
            "text": {
                "content": "Thanks.", 
                "beginOffset": -1
            }, 
            "sentiment": {
                "magnitude": 0.1, 
                "score": 0.1
            }
